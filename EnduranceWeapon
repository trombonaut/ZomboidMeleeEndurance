[System.Serializable]
public class EnduranceWeapon
{
    public string Name;
    public float Weight;
    public float EnduranceMod;
    public float MinDmg;
    public float MaxDmg;
    public float CritChance;
    public float CritBonus;
    public int Kills;
    public float TotalConsecutiveHitBonus;
}
